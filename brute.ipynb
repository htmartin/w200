   "source": [
    "x = float(input('enter a number:'))\n",
    "\n",
    "\n",
    "num_guesses = 0\n",
    "epsilon = .0000000001\n",
    "guess = 0.0\n",
    "\n",
    "while guess**2 <= x:\n",
    "    guess+= epsilon\n",
    "    num_guesses+=1\n",
    "    \n",
    "print( guess, 'is close to the sqrt of ', x)    \n",
    "print('num_guesses =', num_guesses)    "
   ]
